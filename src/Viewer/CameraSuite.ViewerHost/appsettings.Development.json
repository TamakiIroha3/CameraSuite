{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "CameraSuite": {
    "Nats": {
      "Url": "tls://localhost:4222",
      "ClientName": "camera-viewer-dev",
      "ClientCertificatePath": "certs/client.pfx",
      "ClientKeyPassword": "",
      "CaCertificatePath": "certs/ca.crt",
      "TrustAllCertificates": true,
      "RequestTimeoutSeconds": 5
    },
    "Viewer": {
      "MediamtxExecutable": "mediamtx",
      "MediamtxConfigPath": "infra/mediamtx/mediamtx.yaml",
      "ViewerId": "viewer-dev",
      "MaxSimultaneousStreams": 16,
      "UiRefreshRateHz": 30,
      "DisplayMode": "Grid"
    }
  }
}
