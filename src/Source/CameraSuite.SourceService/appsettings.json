{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "CameraSuite": {
    "Source": {
      "SourceId": "source-default",
      "DefaultChannelName": "default",
      "LocalRtmpUrl": "rtmp://127.0.0.1/live",
      "FfmpegExecutable": "ffmpeg",
      "RetryDelaySeconds": 5,
      "MaxRetryCount": 10,
      "ControlPlanePath": "/ws/source",
      "UseTls": false,
      "TrustAllCertificates": true
    }
  }
}

