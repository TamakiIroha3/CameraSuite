{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "CameraSuite": {
    "Nats": {
      "Url": "tls://localhost:4222",
      "ClientName": "camera-source-dev",
      "ClientCertificatePath": "certs/client.pfx",
      "ClientKeyPassword": "",
      "CaCertificatePath": "certs/ca.crt",
      "TrustAllCertificates": true,
      "RequestTimeoutSeconds": 5
    },
    "Source": {
      "SourceId": "source-dev",
      "DefaultChannelName": "demo",
      "LocalRtmpUrl": "rtmp://127.0.0.1/live",
      "FfmpegExecutable": "ffmpeg",
      "RetryDelaySeconds": 5,
      "MaxRetryCount": 10
    }
  }
}
